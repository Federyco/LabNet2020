@model List<Lab.Demo.EF.MVC.Models.AnimeView>
<html>
<head>
    <link rel="stylesheet" href="~/css/estilos.css">
</head>

<body>
    <h2 style="margin-left:850px;">Api</h2>
    <table class="tabla_select">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Level</th>

            </tr>
        </thead>
        <tbody>
            @{foreach (var digimon in Model)
                {
                    var imagen = digimon.Image;
                        <tr>
                            <td>@digimon.Name</td>
                            <td>@digimon.Level</td>
                        </tr>
                }
            }
        </tbody>
    </table>
</body>
</html>